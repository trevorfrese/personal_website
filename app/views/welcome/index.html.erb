<script>
  $(document).ready(function(){
    $('.parallax').parallax();
    $('.carousel').carousel();

    var options = [
      {selector: '#first-title', offset: 300, callback: function() {
        Materialize.showStaggeredList("#first-title");
      } },
      {selector: '#picture-of-me', offset: 300, callback: function() {
        Materialize.fadeInImage('#picture-of-me');
      }},
      {selector: '#description', offset: 300, callback: function() {
        Materialize.showStaggeredList("#description");
      } },
    ];
    Materialize.scrollFire(options);

    $("#js-rotating").Morphext({
      // The [in] animation type. Refer to Animate.css for a list of available animations.
      animation: "flipInX",
      // An array of phrases to rotate are created based on this separator. Change it if you wish to separate the phrases differently (e.g. So Simple | Very Doge | Much Wow | Such Cool).
      separator: ",",
      // The delay between the changing of each phrase in milliseconds.
      speed: 2000,
      complete: function () {
        // Called after the entrance animation is executed.
      }
    });

  });


</script>
